{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"minimium-stability": "dev",
	"require": {
		"laravel/framework": "5.0.*@dev",
		"illuminate/html": "5.0.*@dev",
		"doctrine/dbal":"~2.4"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"X10WeaponStatsApi\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
